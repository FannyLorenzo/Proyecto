<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.AudioActivity">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="578dp"
        app:srcCompat="@drawable/bg_superior"
        tools:layout_editor_absoluteX="-93dp"
        tools:layout_editor_absoluteY="-91dp" />
    <TextView
        android:id="@+id/txt_musica"
        android:layout_width="268dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="642dp"
        android:fontFamily="@font/montserrat"
        android:text="@string/musica"

        android:textColor="#000000"
        android:textSize="26sp" />

    <Button
        android:id="@+id/btn_toLogin"
        android:layout_width="42dp"
        android:layout_height="36dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="644dp"
        android:background="@drawable/ic_perfil"
        android:backgroundTint="@color/purple_App"
        />

    <ImageButton
        android:id="@+id/imgbtn_atras"
        android:layout_width="78dp"
        android:layout_height="51dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="333dp"
        android:layout_marginBottom="680dp"
        android:background="@color/white"
        app:srcCompat="@drawable/ic_regresar" />

    <ListView
        android:id="@+id/music"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="120dp"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="326dp" />


</RelativeLayout>